{
  "name": "curso-nodejs-zero-producao",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint .src/ ./test --ext .ts --no-error-on-unmatched-pattern",
    "lintfix": "eslint .src/ ./test --ext .ts --fix --no-error-on-unmatched-pattern",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "keywords": [
    "node.js",
    "typescript",
    "api"
  ],
  "author": "Valentim Ara√∫jo",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
